/* * PornHub Logo Style * */
.pornhub {
  display: inline;
  overflow: hidden;
  background-color: #000000;
  padding: 2px 0;
  border-radius: 4px;
  font-weight: 600;
  text-transform: capitalize;
  > span {
    padding: 2px 4px;
    margin: 0;
    border-radius: 4px;
  }
  > span:nth-of-type(1) {
    color: #ffffff;
  }
  > span:nth-of-type(2) {
    background-color: #ffa500;
    color: #000000;
  }
}

@import './browsersupport.styl';